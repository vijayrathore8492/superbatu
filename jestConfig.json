{
  "globals": {
    "ts-jest": {
      "diagnostics": {
        "warnOnly": true
      }
    }
  },
  "preset": "@shelf/jest-mongodb",
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}